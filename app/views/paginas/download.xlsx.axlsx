# frozen_string_literal: true

wb = xlsx_package.workbook
wb.add_worksheet(name: 'Pagina') do |sheet|
  sheet.add_row [
    @clase_modelo.human_attribute_name(:texto),
    @clase_modelo.human_attribute_name(:numero),
  ]

  @paginas.each do |pagina|
    sheet.add_row [
      pagina.texto,
      pagina.numero,
    ]
  end
end
